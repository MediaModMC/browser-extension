{
    "name": "MediaMod Integration",
    "description": "View your current song inside Minecraft with support for YouTube, Soundcloud & more!",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "*://localhost/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.soundcloud.com/*"
            ],
            "js": [
                "services/soundcloud/script.ts"
            ]
        }
    ]
}
